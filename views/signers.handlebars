<div class="presentation__signers">
    Hey <em class="name__presentation">{{name}}</em>, here are the all the signers {{#location}}from <em class="name__presentation">{{.}}</em>{{/location}} : 
</div>

<div class="signers">
    {{#each signers}}
        <div class="signer__name">
            {{#if this.url}}
                <a class="name" href="{{this.url}}">{{first_name}} {{last_name}}</a>
            {{else}}
                <p class="name">{{first_name}} {{last_name}}</p>
            {{/if}}
               {{#this.age}}<p class="age"> age: {{.}} </p>{{/this.age}} 
               {{#this.city}}<p class="city">city: <a href="/signers/{{.}}">{{.}}</a></p>{{/this.city}}
        </div>
    {{/each}}
</div>

<a class="go-back" href="/thanks">Go Back</a>
